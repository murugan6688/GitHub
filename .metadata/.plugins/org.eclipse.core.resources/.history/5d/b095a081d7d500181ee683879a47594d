public class LoginPage extends TestBase  {

	below code is used instead of browser.findElement(By.xpath(xpathExpression))
	
	@FindBy(name="username")
	WebElement username;
	
	@FindBy(name="password")
	WebElement password;
	
	
	
	
	
	
	
}
